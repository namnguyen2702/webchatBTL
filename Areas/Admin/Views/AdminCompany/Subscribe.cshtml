@model webchatBTL.Models.CompanySubscription

<h2>Đăng ký goi dịch vụ</h2>

<form asp-action="Subscribe">
    <div class="form-group">
        <label>Chọn gói dịch vụ</label>
        <select asp-for="PlanId" class="form-control" asp-items="@(new SelectList(ViewBag.SubscriptionPlans, "PlanId", "PlanName"))"></select>
    </div>
    <input type="hidden" asp-for="CompanyId" />
    <button type="submit" class="btn btn-primary">Đăng ký</button>
</form>
